{
  "name": "bahamut-bot-ts",
  "version": "2.0.0",
  "description": "Rewrite of Bahamut Bot in TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "typings": "./typings.d.ts",
  "scripts": {
    "build-ts": "rm -rf ./dist/* && tsc",
    "tsc": "tsc -w --outDir dist --rootDir src --skipLibCheck",
    "start_shard": "./node_modules/cross-env/src/bin/cross-env.js NODE_NO_WARNINGS=1 node ./dist/index.js",
    "start_shard_update": "./node_modules/cross-env/src/bin/cross-env.js NODE_NO_WARNINGS=1 npm update && node ./dist/index.js",
    "dev_shard": "cross-env NODE_NO_WARNINGS=1 node ./node_modules/nodemon/bin/nodemon.js --ignore ./cache/ --ignore ./assets/ --ignore ./config/ ./dist/index.js",
    "dev_shard_update": "cross-env NODE_NO_WARNINGS=1 npm install && npm update && node ./node_modules/nodemon/bin/nodemon.js --ignore ./cache/ --ignore ./assets/ --ignore ./config/ ./dist/index.js"
  },
